#Select statement tp retrive the data

SELECT * FROM moviesdatabase.movies_table;

SELECT * FROM moviesdatabase.movies_table WHERE movie_Actor = "Prabhas";

SELECT * FROM moviesdatabase.movies_table WHERE movie_Director = "Sujeeth";

SELECT * FROM moviesdatabase.movies_table WHERE movie_releasedate = "2019";
