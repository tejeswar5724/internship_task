CREATE SCHEMA `moviesdatabase` ;

#Create Table
CREATE TABLE `moviesdatabase`.`movies_table` (
  `movie__ID` INT NOT NULL,
  `movie__Name` VARCHAR(45) NOT NULL,
  `movie__Actor` VARCHAR(45) NOT NULL,
  `movie__Actress` VARCHAR(45) NOT NULL,
  `movie__Director` VARCHAR(45) NOT NULL,
  `movie__releasedate` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`movie__ID`));

#Insert command to add data
INSERT INTO `moviesdatabase`.`movies_table` (`movie_ID`, `movie_Name`, `movie_Actor`, `movie_Actress`, `movie_Director`, `movie_releasedate`) VALUES ('1', 'Maharshi', 'Mahesh Babu', 'Pooja Hegde', 'Vamsi Paidipally', '2019');
